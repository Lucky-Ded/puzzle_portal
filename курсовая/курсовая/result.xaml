<Window x:Class="курсовая.result"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:курсовая"
        mc:Ignorable="d"
        Title="result" Height="450" Width="800" WindowStyle="None">
    <Window.Background>
        <ImageBrush ImageSource="123.jpg"/>
    </Window.Background>
    <Grid>
        <Button x:Name="b7" Content="Назад" HorizontalAlignment="Left" Margin="22,375,0,0" VerticalAlignment="Top" Width="107" Click="B7_Click" Height="47" FontSize="24" Foreground="White" Background="{x:Null}" BorderBrush="{x:Null}"/>
        <ListBox x:Name="list" HorizontalAlignment="Left" Height="274" Margin="43,84,0,0" VerticalAlignment="Top" Width="703" Background="{x:Null}" BorderBrush="White" Foreground="White" SelectionChanged="ListBox_SelectionChanged"/>
        <Label Content="Таблица лидеров" HorizontalAlignment="Left" Margin="179,10,0,0" VerticalAlignment="Top" Height="58" Width="424" FontSize="36" Foreground="White"/>

    </Grid>
</Window>
